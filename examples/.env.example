# Olog Configuration Environment Variables
# Copy this file to .env and set your values

# Base URL of the Olog service
OLOG_BASE_URL=https://olog.example.com:8443

# Client identification string
OLOG_CLIENT_INFO=My Application v1.0

# Whether to verify SSL certificates (true/false)
OLOG_VERIFY_SSL=true

# Request timeout in seconds
OLOG_TIMEOUT=60
